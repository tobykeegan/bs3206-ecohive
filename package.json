{
  "name": "ecohive-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "development": "next dev",
    "build": "NODE_ENV=production next build",
    "start": "next start",
    "prod": "bun run build && npx kill-port 3000 -y && bun run start",
    "production": "bun run prod",
    "lint": "next lint && prettier . --check",
    "format": "prettier . --write",
    "test:gen": "npx playwright codegen localhost:3000/ ",
    "test": "NODE_ENV=production npx playwright test",
    "analyze": "cross-env NODE_ENV=production ANALYZE=true next build",
    "analyze:server": "cross-env NODE_ENV=production BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env NODE_ENV=production BUNDLE_ANALYZE=browser next build"
  },
  "dependencies": {
    "@axe-core/playwright": "^4.8.5",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/inter": "^5.0.16",
    "@mui/icons-material": "^5.15.8",
    "@mui/joy": "^5.0.0-beta.18",
    "@mui/material": "^5.15.7",
    "@next/bundle-analyzer": "^14.1.0",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "bootstrap": "^5.3.2",
    "cross-env": "^7.0.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.3",
    "next": "latest",
    "next-auth": "4.24.5",
    "nodemailer": "^6.9.9",
    "react": "latest",
    "react-bootstrap": "^2.10.0",
    "react-dom": "latest",
    "react-icons": "^5.0.1",
    "sass": "^1.70.0",
    "sharp": "^0.33.2",
    "validator": "^13.11.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.1",
    "@types/node": "^20.11.7",
    "dotenv": "^16.4.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-security": "^2.1.0",
    "prettier": "^3.2.4"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  }
}
